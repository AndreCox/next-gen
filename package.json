    "url": "git+https://github.com/AndreCox/next-gen.git"
  },
  "keywords": [
    "nextjs",
    "template",
    "cli",
    "tool",
    "generator"
  ],
  "dependencies": {
    "decompress": "^4.2.1",
    "download-github-release": "^0.3.2"
  },
  "author": "Andre Cox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndreCox/next-gen/issues"
  },
  "homepage": "https://github.com/AndreCox/next-gen#readme",
  "goBinary": {
    "name": "nextgen",
    "path": "./bin",
